{
  "name": "dexart-stacking",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "generate:messages": "node ./langs/runGenerator.js",
    "predev": "npm run generate:messages",
    "dev": "nuxt",
    "build": "node build-script.js",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js --fix && npm run lint:style --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.{css,scss,stylus,sass,html,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/i18n": "^7.3.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "@saeris/vue-spinners": "^1.0.8",
    "core-js": "^3.25.3",
    "nuxt": "^2.15.8",
    "papaparse": "^5.3.2",
    "rambda": "^7.3.0",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "vue": "^2.7.10",
    "vue-final-modal": "^2.4.3",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "google-spreadsheet": "^3.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0"
  }
}
